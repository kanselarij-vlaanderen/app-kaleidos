PREFIX  ext:  <http://mu.semte.ch/vocabularies/ext/>
PREFIX  public: <http://mu.semte.ch/graphs/public>

WITH public:
DELETE {
  ?s ?p ?o .
}
WHERE
  { ?s  a   ext:Handtekening ;
        ?p  ?o
  } ;
DELETE DATA {
  GRAPH public: {
    ext:Handtekening <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ext:PublicClass .
  }
}

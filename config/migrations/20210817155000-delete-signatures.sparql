PREFIX  ext:  <http://mu.semte.ch/vocabularies/ext/>
PREFIX  public: <http://mu.semte.ch/graphs/public>
PREFIX  nfo: <http://www.semanticdesktop.org/ontologies/2007/03/22/nfo>
PREFIX  person: <http://www.w3.org/ns/person>

WITH public:
DELETE {
  ?s ?p ?o .
} WHERE {
  ?s ext:Handtekening ;
    ?p ?o .
};

WITH public:
DELETE WHERE{ ext:Handtekening  ext:handtekening  nfo:FileDataObject };

WITH public:
DELETE WHERE{ ext:Handtekening  ext:bevoegdePersoon  person:Person };

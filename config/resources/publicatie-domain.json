{
  "version": "0.1",
  "prefixes": {
    "ext": "http://mu.semte.ch/vocabularies/ext/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "schema": "http://schema.org/",
    "pub": "http://mu.semte.ch/vocabularies/ext/publicatie/",
    "dct": "http://purl.org/dc/terms/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "rdfs": "https://www.w3.org/2000/01/rdf-schema#",
    "adms": "http://www.w3.org/ns/adms#",
    "dossier": "https://data.vlaanderen.be/ns/dossier#",
    "euvoc": "http://publications.europa.eu/ontology/euvoc#",
    "tmo": "http://www.semanticdesktop.org/ontologies/2008/05/20/tmo#"
  },
  "resources": {
    "publication-flows": {
      "name": "publication-flow",
      "class": "pub:Publicatieaangelegenheid",
      "attributes": {
        "translate-before": {
          "type": "datetime",
          "predicate": "pub:uitersteVertaling"
        },
        "publish-before": {
          "type": "datetime",
          "predicate": "pub:uiterstePublicatie"
        },
        "publish-date-requested": {
          "type": "datetime",
          "predicate": "pub:gevraagdePublicatie"
        },
        "published-at": {
          "type": "datetime",
          "predicate": "pub:publicatieOp"
        },
        "remark": {
          "type": "string",
          "predicate": "rdfs:comment"
        },
        "opening-date": {
          "type": "date",
          "predicate": "dossier:openingsdatum"
        },
        "closing-date": {
          "type": "date",
          "predicate": "dossier:sluitingsdatum"
        },
        "created": {
          "type": "datetime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "datetime",
          "predicate": "dct:modified"
        }
      },
      "relationships": {
        "case": {
          "predicate": "dossier:behandelt",
          "target": "case",
          "cardinality": "one"
        },
        "status": {
          "predicate": "adms:status",
          "target": "publication-status",
          "cardinality": "one"
        },
        "mode": {
          "predicate": "pub:publicatieWijze",
          "target": "publication-mode",
          "cardinality": "one"
        },
        "urgency-level": {
          "predicate": "pub:urgentieniveau",
          "target": "urgency-level",
          "cardinality": "one"
        },
        "regulation-type": {
          "predicate": "pub:regelgevingType",
          "target": "regulation-type",
          "cardinality": "one"
        },
        "publication-status-change": {
          "predicate": "prov:hadActivity",
          "target": "publication-status-change",
          "cardinality": "one"
        },
        "mandatees": {
          "predicate": "ext:heeftBevoegdeVoorPublicatie",
          "target": "mandatee",
          "cardinality": "many"
        },
        "numac-numbers": {
          "predicate": "pub:identifier",
          "target": "identification",
          "cardinality": "many"
        },
        "subcases": {
          "predicate": "ext:doorloopt",
          "target": "subcase",
          "cardinality": "many"
        },
        "contact-persons": {
          "predicate": "pub:contactPersoon",
          "target": "contact-person",
          "cardinality": "many"
        },
        "identification": {
          "predicate": "adms:identifier",
          "target": "identification",
          "cardinality": "one"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/publicatie-aangelegenheid/"
    },
    "publication-modes": {
      "name": "publication-mode",
      "class": "pub:PublicatieWijze",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "position": {
          "type": "number",
          "predicate": "schema:position"
        }
      },
      "relationships": {
        "publications": {
          "predicate": "pub:publicatieWijze",
          "target": "publication-flow",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/publicatie-wijze/"
    },
    "publication-statuses": {
      "name": "publication-status",
      "class": "pub:Publicatiestatus",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "position": {
          "type": "number",
          "predicate": "schema:position"
        }
      },
      "relationships": {
        "publications": {
          "predicate": "adms:status",
          "target": "publication-flow",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/publicatie-status/"
    },
    "request-activities": {
      "name": "request-activity",
      "class": "pub:AanvraagActiviteit",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "start-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.startdatum"
        },
        "end-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.einddatum"
        }
      },
      "relationships": {
        "subcase": {
          "predicate": "pub:aanvraagVindtPlaatsTijdens",
          "target": "subcase",
          "cardinality": "one"
        },
        "email": {
          "predicate": "pub:requestActivitySubject",
          "comment": "FIXME: predicate should be dct:subject",
          "target": "email",
          "cardinality": "one",
          "inverse": true
        },
        "used-pieces":{
          "predicate": "prov:used",
          "target": "piece",
          "cardinality": "many"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/aanvraag-activiteit/"
    },
    "translation-activities": {
      "name": "translation-activity",
      "class": "pub:VertaalActiviteit",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "start-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.startdatum"
        },
        "end-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.einddatum"
        },
        "due-date": {
          "type": "datetime",
          "predicate": "tmo:dueDate"
        },
        "target-end-date": {
          "type": "datetime",
          "predicate": "tmo:targetEndTime"
        }
      },
      "relationships": {
        "subcase": {
          "predicate": "pub:vertalingVindtPlaatsTijdens",
          "target": "subcase",
          "cardinality": "one"
        },
        "language": {
          "predicate": "pub:doelTaal",
          "target": "language",
          "cardinality": "one"
        },
        "used-pieces":{
          "predicate": "prov:used",
          "target": "piece",
          "cardinality": "many"
        },
        "generated-pieces":{
          "predicate": "dossier:genereert",
          "target": "piece",
          "cardinality": "many"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/vertaal-activiteit/"
    },
    "proofing-activities": {
      "name": "proofing-activity",
      "class": "pub:DrukproefActiviteit",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "start-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.startdatum"
        },
        "end-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.einddatum"
        }
      },
      "relationships": {
        "subcase": {
          "predicate": "pub:drukproefVindtPlaatsTijdens",
          "target": "subcase",
          "cardinality": "one"
        },
        "used-pieces":{
          "predicate": "prov:used",
          "target": "piece",
          "cardinality": "many"
        },
        "generated-pieces":{
          "predicate": "dossier:genereert",
          "target": "piece",
          "cardinality": "many"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/drukproef-activiteit/"
    },
    "publication-activities": {
      "name": "publication-activity",
      "class": "pub:PublicatieActiviteit",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "start-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.startdatum"
        },
        "end-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.einddatum"
        }
      },
      "relationships": {
        "subcase": {
          "predicate": "pub:publicatieVindtPlaatsTijdens",
          "target": "subcase",
          "cardinality": "one"
        },
        "used-pieces":{
          "predicate": "prov:used",
          "target": "piece",
          "cardinality": "many"
        },
        "generated-pieces":{
          "predicate": "dossier:genereert",
          "target": "piece",
          "cardinality": "many",
          "_comment": "this should be a besluit:Besluit"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/publicatie-activiteit/"

    },
    "cancellation-activities": {
      "name": "cancellation-activity",
      "class": "pub:AnnulatieActiviteit",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "start-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.startdatum"
        },
        "end-date": {
          "type": "datetime",
          "predicate": "dossier:Activiteit.einddatum"
        }
      },
      "relationships": {
        "subcase": {
          "predicate": "pub:annuleringVindtPlaatsTijdens",
          "target": "subcase",
          "cardinality": "one"
        },
        "email": {
          "predicate": "pub:cancellationActivitySubject",
          "comment": "FIXME: predicate should be dct:subject",
          "target": "email",
          "cardinality": "one",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/annulatie-activiteit/"
    },
    "languages": {
      "name": "language",
      "class": "euvoc:Language",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "priority": {
          "type": "number",
          "predicate": "ext:priority"
        }
      },
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/taal/",
      "features": ["include-uri"]
    },
    "urgency-levels": {
      "name": "urgency-level",
      "class": "pub:Urgentieniveau",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "position": {
          "type": "number",
          "predicate": "schema:position"
        }
      },
      "relationships": {
        "publications": {
          "predicate": "pub:urgentieniveau",
          "target": "publication-flow",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/urgentieniveau/"
    },
    "regulation-types": {
      "name": "regulation-type",
      "class": "ext:RegelgevingType",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "scope-note": {
          "type": "string",
          "predicate": "skos:scopeNote"
        },
        "position": {
          "type": "number",
          "predicate": "schema:position"
        },
        "alt-label": {
          "type": "string",
          "predicate": "skos:altLabel"
        }
      },
      "relationships": {
        "publication-flows": {
          "predicate": "pub:regelgevingType",
          "target": "publication-flow",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/concept/regelgeving-type/"
    },
    "publication-status-changes": {
      "name": "publication-status-change",
      "class": "pub:PublicatieStatusWijziging",
      "attributes": {
        "started-at": {
          "type": "datetime",
          "predicate": "prov:startedAtTime"
        }
      },
      "relationships": {
        "publication": {
          "predicate": "prov:hadActivity",
          "target": "publication-flow",
          "cardinality": "one",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/publicatie-status-wijziging/"
    }
  }
}

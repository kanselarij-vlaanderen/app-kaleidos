    "signinghub": "http://mu.semte.ch/vocabularies/ext/signinghub/"
  },
  "resources": {
    "sign-flows": {
      "name": "sign-flow",
      "class": "sign:HandtekenAangelegenheid",
      "attributes": {
        "short-title": {
          "type": "string",
          "predicate": "dct:alternative"
        },
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "opening-date": {
          "type": "date",
          "predicate": "dossier:openingsdatum"
        },
        "closing-date": {
          "type": "date",
          "predicate": "dossier:sluitingsdatum"
        }
      },
      "relationships": {
        "regulation-type": {
          "predicate": "sign:regelgevingType",
          "target": "regulation-type",
          "cardinality": "one"
        },
        "status": {
          "predicate": "adms:status",
          "target": "sign-status",
          "cardinality": "one"
        },
        "case": {
          "predicate": "sign:behandeltDossier",
          "target": "case",
          "cardinality": "one"
        },
        "sign-subcase": {
          "predicate": "sign:doorlooptHandtekening",
          "target": "sign-subcase",
          "cardinality": "one"
        },
        "decide-activity": {
          "_comment": "TODO: split in decide-activity & agenda-item-treatment",
          "predicate": "sign:heeftBeslissing",
          "target": "agenda-item-treatment",
          "cardinality": "one"
        },
        "councelor": {
          "predicate": "dct:creator",
          "target": "person",
          "cardinality": "one"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/handteken-aangelegenheid/"
    },
    "sign-statuses": {
      "name": "sign-status",
      "class": "sign:HandtekenStatus",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "position": {
          "type": "number",
          "predicate": "schema:position"
        }
      },
      "relationships": {
        "sign-flows": {
          "predicate": "adms:status",
          "target": "sign-flow",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/handteken-status/"
    },

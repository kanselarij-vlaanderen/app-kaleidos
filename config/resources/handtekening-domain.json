          "target": "agenda-item-treatment",
          "cardinality": "one"
        },
        "creator": {
          "predicate": "dct:creator",
          "target": "user",
          "cardinality": "one"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://themis.vlaanderen.be/id/handtekenaangelegenheid/"

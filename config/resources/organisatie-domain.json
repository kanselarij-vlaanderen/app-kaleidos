{
  "version": "0.1",
  "prefixes": {
    "dct": "http://purl.org/dc/terms/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "ext": "http://mu.semte.ch/vocabularies/ext/"
  },
  "resources": {
    "organizations": {
      "name": "organization",
      "class": "org:Organization",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "created": {
          "type": "datetime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "datetime",
          "predicate": "dct:modified"
        },
        "identifier": {
          "type": "string",
          "predicate": "org:identifier"
        }
      }
    },
    "relationships": {
      "user": {
        "predicate": "ext:modifiedBy",
        "target": "modified-by",
        "cardinality": "one"
      },
      "contact-persons": {
        "predicate": "org:memberOf",
        "target": "contact-point",
        "inverse": true
      }
    },
    "features": ["include-uri"],
    "resource-base": "https://data.vlaanderen.be/id/organisatie"
  }
}